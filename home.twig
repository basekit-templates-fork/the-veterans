
{% extends "layout.twig" %}

{% block content %}

<header class="bk-header clearfix">
      <div class="headerwrapper">
        <div class="logo-container">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            {{widget('companyname', 'companyname', {"business": "ADVENTURE"})|raw}}
        </div>
        <div class="textwidget">
            {{widget('content', 'textwidgetbox1', {content: '<span>"Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmodtempor incididunt ut"</span>', lines: 'all'})|raw}}
        </div>
       </div>
  </header>

{{widget('feature', 'featurehome', {"header": "profile", "strapline":"profile", "buttonText":"Lorem Ipsum","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': false})|raw}} 

<div class="navbar clearfix">
    <div class="left">
        {{widget('extendednavigation', 'navigationheader', {"align":"left"})|raw}}
    </div>
    <div class="right">
        {{widget('profile', 'socialicons', { 'profileType' : 'socialicons' })|raw}}
    </div>
</div>

<div class="page-wrap">
    <div class="headerwrapper">
        <div class="mainzone headerwrapper">
		        {{zone('main')|raw}}
        </div>
    </div>
</div>

<footer class="bk-footer">
      <div class="headerwrapper">
          <div class="left">
          {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
              {{widget('companyname', 'thisunqiuewidgetname', {"business": "profile"})|raw}}
          </div>
          <div class="right">
              {{widget('signupform', 'thisunqiuewidgetname', {'text': 'Send', 'label': 'Your Email', 'formTitle': 'Signup Form', 'email': 'profile'})|raw}}
          </div>
      </div>
</footer>

{% endblock %}